{
  "name": "react-boilerplate-typescript",
  "version": "1.0",
  "author": "VietNa",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --progress",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:coverage": "jest --verbose --coverage",
    "build:dev": "webpack --watch --hot --progress",
    "start": "webpack -p --progress --optimize-max-chunks 15 --optimize-min-chunk-size 10000 --env.prod=true",
    "eslint": "./node_modules/.bin/eslint --ext .jsx,.js src/",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:ts",
    "lint:css": "stylelint './app/**/*.{js,ts}'",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:ts": "npm run lint:tslint",
    "lint:tslint": "tslint --config ./tslint.json --project ./tsconfig.json --fix && npm run typecheck",
    "typecheck": "tsc --noEmit",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mrnav90/react-boilerplate-typescript.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.15.1"
  },
  "eslintIgnore": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add --force"
    ],
    "*.ts": "npm run lint:tslint",
    "*.tsx": "npm run lint:tslint"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "yarn eslint"
    }
  },
